#!/bin/bash
arg1=$1
arg2=$2
if [ $arg1 = 1 ]
then
	./gensine MONO 697 16 32000 32767 $arg2 > gentone1.su
	./gensine MONO 1209 16 32000 32767 $arg2 > gentone2.su
	./mix gentone1.su 1.0 gentone2.su 1.0
	rm gentone1.su
	rm gentone2.su
elif [ $arg1 = 2 ]
then
	./gensine MONO 697 16 32000 32767 $arg2 > gentone1.su
	./gensine MONO 1336 16 32000 32767 $arg2 > gentone2.su
	./mix gentone1.su 1.0 gentone2.su 1.0
	rm gentone1.su
	rm gentone2.su
elif [ $arg1 = 3 ]
then
	./gensine MONO 697 16 32000 32767 $arg2 > gentone1.su
	./gensine MONO 1477 16 32000 32767 $arg2 > gentone2.su
	./mix gentone1.su 1.0 gentone2.su 1.0
	rm gentone1.su
	rm gentone2.su
elif [ $arg1 = 4 ]
